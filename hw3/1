def can_eat(c1,c2):
#эт функция у нас с картежиками или как их там 
    x1=c1[0]
    y1=c1[1]
    x2=c2[0]
    y2=c2[1]
#создаем супер пупер переменные которые присвоены каждому из картежей и обозначаем их места в тех НЕИЗМЕНЯЕМЫХ СПИСКАХ
    if abs(x2-x1)+abs(y2-y1)==3:
        return True
#РЕТЕРН этот значит что это принт для функции ПРИКИНТЕ?!?!!?!?!
    else:
        return False
#а теперь супер принт и все готово
print ('result = ', can_eat((2,1),(4,2)))
#ну а теперь удачи ВАМ вводить все значения для тестов потому что когда я это все писала я не понимала как это сделать и мне стало плохо СПАСИБО ВАМ ОГРОМНОЕ